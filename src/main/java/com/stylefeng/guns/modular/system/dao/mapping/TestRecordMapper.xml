<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.stylefeng.guns.modular.system.dao.TestRecordMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.stylefeng.guns.modular.system.model.TestRecord">
        <result column="name" property="name" />
        <result column="testname" property="testname" />
        <result column="testscore" property="testscore" />
        <result column="testtime" property="testtime" />
        <result column="testip" property="testip" />
        <result column="sex" property="sex" />
        <result column="age" property="age" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        name, testname, testscore, testtime, testip, sex, age
    </sql>

</mapper>
